
.msg-btn{
  color: #666666;
  font-size: 26rpx;
  display: inline-block;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #FAFAFA;
  border: 1rpx solid rgba(229,229,229,1);
  border-radius: 8rpx;
  text-align: center;
}

.xy-Rule{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  line-height: 50rpx;
  margin-top: 15rpx;
  margin-bottom: 32rpx;
  box-sizing: border-box;
  color: #9B9B9B;
}
.xy-Rule>view{
  display: flex;
  align-items: center;
}
.xy-Rule .image{
  height: 24rpx;
  width: 24rpx;
  border-radius:4rpx;
  margin-top: -21rpx;
  margin-right: 14rpx;
}
.xy-Rule .image image{
  height: 24rpx;
  width: 24rpx;
  border-radius:4rpx;
}
.xy-Rule .image .icon-border{
  display: inline-block;
  height: 20rpx;
  width: 20rpx;
  border-radius:4rpx;
  border:2rpx solid rgba(208,208,208,1);
}
.xy-Rule navigator{
  color: #1A43A9;
}

