page{
  padding:0 30rpx;
}

.list{
  margin-top: 18rpx;
}

.list-tab{
  position: sticky;
  top: 0;
  left: 0;
  z-index: 1;
  background-color: #ffffff;
  height: 95rpx;
  border-bottom: 1rpx solid rgba(235,235,235,1);
}

.list-tab_left{
  color: #000000;
  font-size: 34rpx;
  font-weight: bold;
}

.list-tab_right{
  font-size: 26rpx;
  color: #999999;
  margin-right: 15rpx;
}

.list-tab_right image {
  width: 14rpx;
  height: 22rpx;
}

.online{
  width:100%;
  height:10rpx;
  background-color: #fff;
  transform: translateY(-2rpx);
}