.tips{
  font-size:26rpx;
  color: #035FC2;
  margin:8rpx 0rpx 0rpx 38rpx; 
}

/* -------------- 单选样式 ------------------ */
.label .select-wrapper{
  width:50%;
  font-size:34rpx;
}
.radio {
  width:26rpx;
  height:26rpx;
  margin-right:18rpx;
  border-radius:50%;
  background:#fff;
  border:2rpx solid #B0B0B0;
  transition:all 0.3s linear;
  transform:translateY(3rpx);
}

.radio .circle {
  width:0rpx;
  height:0rpx;
  border-radius:50%;
  background:#035FC2;
  transition:all 0.3s linear;
}

.select{
  border:2rpx solid #035FC2;
}

.select .circle{
  width:14rpx;
  height:14rpx;
}
